<h1>Patient Log In</h1>

<%= form_tag matched_path do %>
	<div class="field">
		<%= label_tag :phone_number%>
		<%= text_field_tag :phone_number, params[:phone_number] %>
	</div>
	<div class="field">
			<%= label_tag :password %>
			<%= password_field_tag :password %>
	</div>
	<div class="actions"><%= submit_tag "Log In" %></div>
	<% end %>