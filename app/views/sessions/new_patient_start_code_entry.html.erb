<h1>New Patient Log In</h1>

<%= form_tag patient_code_path do %>
	<div class="field">
		<%= label_tag :phone_number%>
		<%= text_field_tag :phone_number, params[:phone_number] %>
	</div>
	<div class="field">
			<%= label_tag :start_code %>
			<%= password_field_tag :start_code, params[:start_code] %>
	</div>
	<div class="actions"><%= submit_tag "Log In" %></div>
	<% end %>